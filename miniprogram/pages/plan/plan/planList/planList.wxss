/* pages/plan/plan/planList/planList.wxss */
page {
  background-color: whitesmoke;
}
li {
  display: block;
  height: 50px;
  line-height: 80px;
  border-bottom: 1px solid lightpink;
  border-radius: 15px;
  padding:0 15px;
  display: flex;
  justify-content: space-between;
}
li:hover{
  background-color: lightpink;
}
.fixed{
  position: fixed;
}
.bottom-container{
  width: 100%;
  height: 80px;
  line-height: 80px;
  text-align: center;
  bottom: 0;
  background-color: whitesmoke;
}